{
    "functions": {
        "source": "."
    },
    "hosting": {
        "public": "./dist",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "*",
                "function": "index"
            },
            {
                "source": "!(public)/**",
                "function": "index"
            }
        ]
    }
}